pipeline {
    agent any 

    stages {
        stage("pushing") {
            steps {
                withCredentials([usernamePassword(credentialsId: 'github', usernameVariable: 'USERNAME', passwordVariable: 'PASSWORD')]) {
                    script {
                        echo "Username: ${USERNAME}"
                        echo "Password: ${PASSWORD}"
                        sh 'cd /home/devesh/trial && git diff'
                    }
                }
            }
        }
    }
}
